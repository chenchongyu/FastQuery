<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="dimension" name="actionBarHeight" />

    <declare-styleable name="CircleTextView">
        <attr name="border_width" format="dimension"/>
        <attr name="radius" format="integer"/>
        <attr name="text" format="string"/>
        <attr name="border_color" format="color|reference"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="SearchView">
        <attr name="hint" format="string|reference"/>
        <attr name="input_type">
            <enum name="text" value="1"/>
            <enum name="number" value="2"/>
            <enum name="password" value="128"/>
        </attr>
    </declare-styleable>
</resources>